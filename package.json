{
  "name": "gen-tree-app",
  "main": "index.js",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "generate:icons": "echo 'skip generate icons'",
    "prebuild": "yarn run generate:icons",
    "start": "yarn run prebuild && expo start",
    "android": "yarn run prebuild && expo start --android",
    "ios": "yarn run prebuild && expo start",
    "stop": "pkill -f 'expo start'",
    "web": "yarn run prebuild && expo start --web",
    "test": "jest --config jest.config.js",
    "lint": "eslint ./src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
    "forma": "yarn format",
    "tsprune": "ts-prune --skip || true",
    "ci": "yarn format && yarn prettier --write ./src  && yarn lint && yarn test && (npx expo-doctor || true)",
    "build-dev-android": "eas build --profile development --platform android",
    "build-preview-android": "eas build --profile preview --platform android",
    "build-prod-android": "eas build --profile production --platform android",
    "build-prod-ios": "eas build --profile production --platform ios"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "expo": "~52.0.46",
    "expo-build-properties": "~0.13.3",
    "expo-document-picker": "~13.0.3",
    "expo-file-system": "~18.0.12",
    "expo-image-picker": "~16.0.6",
    "expo-localization": "~16.0.1",
    "expo-sharing": "~13.0.1",
    "i18next": "^23.11.5",
    "jszip": "^3.10.1",
    "react": "18.3.1",
    "react-i18next": "^15.0.2",
    "react-native": "0.76.9",
    "react-native-dotenv": "3.4.11",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-svg": "15.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@testing-library/react-native": "^13.2.0",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-complexity": "^1.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "29.7.0",
    "jest-expo": "~52.0.6",
    "prettier": "^3.6.2",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.3.1",
    "ts-prune": "^0.10.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.46.3"
  },
  "resolutions": {
    "metro": "^0.81.0",
    "metro-resolver": "^0.81.0",
    "metro-config": "^0.81.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-zip-archive"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
